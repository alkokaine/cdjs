{
  "name": "cdjs",
  "version": "0.0.6",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-examples": "vue-cli-service build --dest dist/cdjs-doc --target app ./src/main.js",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint --fix",
    "build:lib": "vue-cli-service build --target lib --dest dist/cdjs --name cdjs ./src/index.js"
  },
  "main": "./src/index.js",
  "files": [
    "src/common/*",
    "src/components/*",
    "src/index.js",
    "dist/**"
  ],
  "dependencies": {
    "axios": "^0.24.0",
    "bootstrap-icons": "^1.7.1",
    "core-js": "^3.6.5",
    "element-ui": "^2.15.6",
    "vue": "^2.6.11",
    "vue-debounce": "^3.0.1",
    "vue-router": "^3.5.3"
  },
  "devDependencies": {
    "@popperjs/core": "^2.10.2",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/test-utils": "^1.3.0",
    "babel-eslint": "^10.1.0",
    "bootstrap": "^5.1.3",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "isomorphic-fetch": "^3.0.0",
    "jest-extended": "^1.2.0",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "v-tooltip": "^3.0.0-beta.4",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
